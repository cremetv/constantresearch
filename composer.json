{
  "require": {
    "craftcms/cms": "^3.4.25",
    "craftcms/contact-form": "^2.2",
    "craftcms/contact-form-honeypot": "^1.0",
    "craftcms/redactor": "^2.7",
    "ether/seo": "^3.6",
    "hybridinteractive/craft-contact-form-extensions": "^1.2",
    "miranj/craft-obfuscator": "^1.0",
    "nystudio107/craft-imageoptimize": "^1.6",
    "olivestudio/craft-olivemenus": "^1.2",
    "utakka/redactor-anchors": "^1.1",
    "vlucas/phpdotenv": "^3.4.0"
  },
  "require-dev": {
    "yiisoft/yii2-shell": "^2.0.3"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "7.0"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
